{
    "$schema": "https://typedoc.org/schema.json",
    "entryPoints": ["./src/index.ts", "./src/helpers/NoteBuilder.ts", "./src/helpers/PollBuilder.ts"],
    "projectDocuments": ["./documents/*.md"],
    "plugin": ["typedoc-plugin-missing-exports", "typedoc-plugin-mdn-links"],
    "placeInternalsInOwningModule": true,
    "name": "Cardboard",
    "excludeExternals": true,
    "hideGenerator": true,
    "out": "docs",
    "sidebarLinks": {
        "Github": "https://github.com/kitsu-org/cardboard"
    },
    "navigation": {
        "includeCategories": false,
        "includeGroups": false,
        "includeFolders": false
    },
    "categorizeByGroup": false
}
